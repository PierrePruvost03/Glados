Inklude fmod (fmod)
Inklude ceil (ceil)
Inklude floor (floor)

Funk round(Float a) -> Float {
    Int sign = 1;
    Float modulo;
    If (a `<` 0) {
        sign = -1;
    }
    modulo = fmod(a, sign);
    If (modulo `>=` 0.5) {
        Return ceil(a);
    } Else If (modulo `<=` -0.5) {
        Return floor(a);
    } Else If (modulo `<` 0) {
        Return ceil(a);
    } Else {
        Return floor(a);
    }
}
